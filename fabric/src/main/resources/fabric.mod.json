{
  "description": "Multiversion API & mod loader with helpers to do things deemed impossible",
  "depends": {
    "fabricloader": ">=0.14.0",
    "fabric": "*",
    "minecraft": ">=1.16.5",
    "java": ">=8"
  },
  "entrypoints": {
    "preLaunch": [
      {
        "adapter": "multiversionAdaptor",
        "value": "mods.thecomputerizer.theimpossiblelibrary.fabric.core.TILLoadingPluginFabric"
      }
    ]
  },
  "environment": "*",
  "icon": "logo.png",
  "id": "theimpossiblelibrary",
  "languageAdapter": {
    "multiversionAdaptor": "mods.thecomputerizer.theimpossiblelibrary.fabric.core.TILLanguageAdaptor"
  },
  "name": "${name}",
  "schemaVersion": 1,
  "version": "${version}"
}