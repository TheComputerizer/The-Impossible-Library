{
  "schemaVersion": 1,
  "custom": {
    "modmenu": {
      "badges": [
        "library"
      ]
    }
  },
  "description": "Multiversion loader for The Impossible Library disguised as a language adaptor",
  "depends": {
    "fabricloader": ">=0.14.0",
    "fabric": "*",
    "minecraft": ">=1.16.5",
    "java": ">=8"
  },
  "entrypoints": {
    "preLaunch": [
      {
        "adapter": "multiversionAdaptor",
        "value": "${injector}"
      }
    ],
    "mm:early_risers": [
      "${coremods}"
    ]
  },
  "environment": "*",
  "icon": "logo.png",
  "id": "theimpossibleloader",
  "languageAdapters": {
    "multiversionAdaptor": "${adaptor}"
  },
  "mixins": [
    "mixins.mm.json",
    "theimpossiblelibrary.mixins.json"
  ],
  "name": "The Impossible Loader",
  "version": "${version}"
}